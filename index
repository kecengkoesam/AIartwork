<script>
document.addEventListener("DOMContentLoaded", () => {
  // Sidebar
  const menuBtn=document.getElementById('menu-button');
  const sidebar=document.getElementById('sidebar');
  const galleryToggle=document.getElementById('gallery-toggle');
  const gallerySubmenu=document.getElementById('gallery-submenu');
  let galleryOpen=false;

  if(menuBtn && sidebar){
    menuBtn.addEventListener('click',()=>{
      sidebar.classList.toggle('open');
      menuBtn.classList.toggle('open');
    });
  }

  if(galleryToggle && gallerySubmenu){
    galleryToggle.addEventListener('click',(e)=>{
      e.preventDefault();
      galleryOpen=!galleryOpen;
      gallerySubmenu.style.display=galleryOpen?'flex':'none';
    });
  }
  
  // PIN untuk menu Foto
     const PIN_PRIVATE = "020711"; // <-- ganti sesuai keinginanmu
     const privateLink = document.getElementById("private-link");
  
     const pinModal = document.getElementById("pinModal");
     const pinInput = document.getElementById("pinInput");
     const pinOk = document.getElementById("pinOk");
     const pinCancel = document.getElementById("pinCancel");
  
     if(privateLink){
      privateLink.addEventListener("click",(e)=>{
      e.preventDefault();
      pinInput.value = "";
      pinModal.style.display = "flex"; // buka modal
      pinInput.focus();
     });
  }
  
  pinOk.addEventListener("click",()=>{
     if(pinInput.value === PIN_PRIVATE){
      pinModal.style.display = "none";
      showPage('album-list');
     } else {
      alert("PIN salah! Akses ditolak.");
      pinInput.value = "";
     }
  });
  
  pinCancel.addEventListener("click",()=>{
     pinModal.style.display = "none";
  });
  
  // tutup modal jika klik di luar konten
  window.addEventListener("click",(e)=>{
     if(e.target === pinModal){
      pinModal.style.display = "none";
     }
  });

  // SPA navigation
  window.showPage=function(id){
    document.querySelectorAll('section').forEach(sec=>sec.classList.remove('active'));
    document.getElementById(id).classList.add('active');
    sidebar.classList.remove('open');
    menuBtn.classList.remove('open');
  };

  // Init albums
  function initAlbum(name,photos){
    const gallery=document.getElementById(`gallery-${name}`);
    const lightbox=document.getElementById(`lightbox-${name}`);
    const lightboxImg=document.getElementById(`lightbox-img-${name}`);
    const prevBtn=document.getElementById(`prevBtn-${name}`);
    const nextBtn=document.getElementById(`nextBtn-${name}`);
    let currentIndex=0;
    photos.forEach((src,i)=>{
      const img=document.createElement('img');
      img.src=src;
      img.onclick=()=>{currentIndex=i;showLightbox();};
      gallery.appendChild(img);
    });
    function showLightbox(){lightbox.style.display='flex';lightboxImg.src=photos[currentIndex];}
    prevBtn.onclick=()=>{currentIndex=(currentIndex-1+photos.length)%photos.length;showLightbox();};
    nextBtn.onclick=()=>{currentIndex=(currentIndex+1)%photos.length;showLightbox();};
  }
  window.closeLightbox=function(name){document.getElementById(`lightbox-${name}`).style.display='none';}

  // Album photos
  
const photosCitra = [
